<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @{
        List<string> myList = new List<string>(){
                "Apple Pie",
                "Burritors",
                "Clams Casino",
                "Donuts",
            }; 
    }
    <h1>Here are some delicious foods!</h1>
    <ul>
        @foreach(string item in myList){
            <li>@item</li>
        }
    </ul>  
</body>
</html>