@using ViewModelFun.Models
@model List<User>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="wrapper">
        <h1>Users</h1>
        <h3>Here are some Users!</h3>
        <div class="content">
            @foreach (var item in Model)
            {
                <p>@item.name</p>
            }
        </div>
    </div>
</body>
</html>