@model Array

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="wrapper">
        <h1>Here are some numbers!</h1>
        <div class="content">
            @foreach (var item in Model)
            {
              <p>@item</p>
            }
        </div>
    </div>
</body>
</html>